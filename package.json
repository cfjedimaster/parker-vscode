{
    "name": "parker-code-extension",
    "displayName": "Run Parker on a CSS file",
    "description": "Uses the Parker CSS analysis tool within Code.",
    "version": "0.0.1",
    "publisher": "raymondcamden",
	"bugs": {
		"url": "https://github.com/Microsoft/vscode-extension-samples/issues",
		"email": "raymondcamden@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples.git"
	},
    "homepage": "https://github.com/Microsoft/vscode-extension-samples/tree/master/textdocumentprovider-sample/README.md",
	"categories": [
		"Linters"
	],
    "engines": {
        "vscode": "^0.10.7"
    },
    "activationEvents": [
        "onCommand:extension.showParkerAnalysis"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.showParkerAnalysis",
                "title": "Show Parker Analysis"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "tslint": "tslint -c tslint.json src/extension.ts"
    },
    "devDependencies": {
        "typescript": "^1.7.5",
        "vscode": "^0.11.0"
    },
    "dependencies": {
        "parker":"0.0.10"
    }
}